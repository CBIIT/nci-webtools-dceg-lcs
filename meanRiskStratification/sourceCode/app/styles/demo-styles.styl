red=#A90101

.red
  background-color red

#main
  width 1100px
  margin 0 auto

.title
  background lightgrey

table input[type="text"]
    width 50px

#portal-footer
  clear both
  border-top 1px solid black
  line-height 1.2em
  margin 2em 0em 1em 0em
  padding 1em 0em 1em 0em
  text-align center
  background-color #FFF
  color #333333
  font-size 79%

.top-header
  text-align center
  color #FFF

.header
  background-color #AAA
  color #444

.panel-toggle a,.panel-title a
  color black

.well,#results
  min-height 645px

#markers
  th.header
    font-size small

  .panel-body
    background linear-gradient(to bottom, #feffff 0%, #feffff 50%, #e7f3fa 100%) repeat scroll 0 0 rgba(0,0,0,0)
.termToDefine, th
  font-weight bold