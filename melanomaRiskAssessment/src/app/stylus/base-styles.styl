form
  display block
  > ul
    list-style none
  li
    margin 5px 0px
    overflow hidden
    > label
      display inline-block
      float left
      width 5em
    > p
      box-sizing border-box
      display inline-block
      float left
      margin 0px -13em 0px -5em
      padding 0px 13em 0px 5em
      width 100%
    > .input
      display inline-block
      width 13em
      select
        width 100%
.female, .male
  &:not(.show)
    display none
.no-css
  display none
